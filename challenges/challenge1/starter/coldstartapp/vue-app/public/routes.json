{
  "routes": [
    {
      "route": "/api/catalog"
    },
    {
      "route": "/login",
      "serve": "/.auth/login/github"
    },
    {
      "route": "/logout",
      "serve": "/.auth/logout"
    },
    {
      "route": "/*",
      "serve": "/index.html",
      "statusCode": 200,
      "allowedRoles": [
        "authenticated"
      ]
    }
  ],
  "platformErrorOverrides": [
    {
      "errorType": "NotFound",
      "serve": "/404.html"
    }
  ]
  
}
